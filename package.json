{
  "name": "edge-api",
  "version": "0.1.0",
  "description": "Edge API router with user authentication and database middleware",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\" \"*.{md,mdx}\"",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "edge",
    "api",
    "router",
    "middleware",
    "authentication",
    "database"
  ],
  "author": "AI Primitives",
  "license": "MIT",
  "homepage": "https://mdx.org.ai",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ai-primitives/edge-api.git"
  },
  "bugs": {
    "url": "https://github.com/ai-primitives/edge-api/issues"
  },
  "dependencies": {
    "@auth/core": "^0.37.4",
    "@mdxdb/fetch": "github:ai-primitives/mdxdb#main",
    "itty-router": "^4.0.0",
    "next-auth": "5.0.0-beta.25"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/itty-router-extras": "^0.4.3",
    "@types/next-auth": "^3.15.0",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "semantic-release": "^24.2.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
